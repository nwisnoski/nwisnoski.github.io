SPCmod <- function(t, x, parms) {
with(as.list(c(parms, x)), {
import <- sigimp(t)
dS <- import - b*S*P + g*C     #substrate
dP <- c*S*P  - d*C*P           #producer
dC <- e*P*C  - f*C             #consumer
res <- c(dS, dP, dC)
list(res)
})
}
